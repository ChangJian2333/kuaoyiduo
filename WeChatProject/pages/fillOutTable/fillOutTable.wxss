/* fillOutTable.wxss */
.content {
  border: 0px solid darkgray;
}

.header {
  margin-left: 0px;
  margin-right: 0px;
  margin-top: 0px;
}

.header-title {
  position: absolute;
  top: 20px;
  left: 20px;
  right: 20px;
  height: 30px;
  text-align: center;
}
.regist-title {
  margin-left: 10px;
  margin-right: 10px;
  margin-top: 10px;
  margin-bottom: 20px;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 18px;
}

.regist-title-left {
  width: 45%;
  height: 50px;
  background: #09bb07;
  color: #fff;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;  
  margin-right: 5%;
}

.regist-title-right {
  width: 45%;
  height: 50px;
  background: #09bb07;
  color: #fff;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;  
  margin-left: 5%;
}
.table {
  margin-left: 10px;
  margin-right: 10px;
  margin-top: 60px;
}
.tr-header {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 40px;
  align-items: center;
  font-size: 17px;
}
.tr-list {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 40px;
  align-items: center;
  font-size: 17px;
}
.td-header {
  width: 50%;
  justify-content: center;
  background: #09bb07;
  display: flex;
  height: 40px;
  color: #fff;
  align-items: center;
}

.td-list {
    width:50%;
    justify-content: center;
    text-align: center;
    font-size: 17px;
}

.td-list-link {
    width:50%;
    justify-content: center;
    text-align: center;
    color: #09bb07;
    font-size: 17px;
}

.bg-g{
  background: #E6F3F9;
}
.th {
  width: 35%;
  justify-content: center;
  background: #09bb07;
  display: flex;
  height: 40px;
  color: #fff;
  align-items: center;
}

.th-data {
  width: 35%;
  justify-content: center;
  background: #fff;
  display: flex;
  height: 40px;
  align-items: center;
}