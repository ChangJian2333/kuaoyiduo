/* fillOutTable.wxss */

.content {
  border: 0px solid darkgray;
}

.header {
  margin-left: 0px;
  margin-right: 0px;
  margin-top: 0px;
}

.header-title {
  position: absolute;
  top: 20px;
  left: 20px;
  right: 20px;
  height: 30px;
  text-align: center;
}
.save-title {
  margin-left: 10px;
  margin-right: 10px;
  margin-top: 10px;
  margin-bottom: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 35px;
  font-size: 17px;
  background: #09bb07;
  color: #ffffff;
  border-color: #09bb07;
  border-width: 0.5px;
  border-style: solid;
  border-radius: 4px;
}
.table {
  margin-left: 10px;
  margin-right: 10px;
  margin-top: 10px;
}
.tr-header {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 35px;
  align-items: center;
}
.tr-list {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 35px;
  align-items: center;
}
.td-header {
  width: 50%;
  justify-content: center;
  background: #09bb07;
  display: flex;
  height: 35px;
  color: #fff;
  align-items: center;
  font-size: 16px;
}
.font-input{
  font-size: 16px;
}
.td-list {
    width:50%;
    justify-content: center;
    text-align: center;
    font-size: 16px;
}

.bg-g{
  background: #E6F3F9;
}
