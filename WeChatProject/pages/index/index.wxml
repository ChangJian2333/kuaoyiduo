<!--index.wxml-->
  <view  class="userinfo">
    <image bindtap="pushDetailPage" class="userinfo-avatar" src="{{avatarUrl}}" background-size="cover"></image>
    <label class="userinfo-nickname">{{nickName}}</label>
  </view>
  <view class="content">
   <block wx:for="{{listData}}">
     <view class="content-cell"> 
      <button class="content-cellText" bindtap='buttonClick' data-tag="{{index}}">{{item.text}}</button>
     </view> 
   </block>
  </view>
