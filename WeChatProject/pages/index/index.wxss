/**index.wxss**/
.userinfo {
  margin-top: 30px;
  display: flex;
  flex-direction: column; 
  align-items: center;  
}

.userinfo-avatar {
  width: 90px;
  height: 90px;
  margin: 10px;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}
.content{
  display: flex;
  flex-direction: column; 
  align-items: center;  
  margin-left: 10px;
  margin-right: 10px;
  margin-top: 10px;
  margin-bottom: 10px;
}
.content-cell{
width: 100%;
height: 50px;
font-size: 17px;
background: #E6F3F9;
}